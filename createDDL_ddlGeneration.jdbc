CREATE TABLE tbl_adresse (fldAdr_id INTEGER NOT NULL, fld_codePostal VARCHAR(255), fld_streetName VARCHAR(255), fld_streetNumber INTEGER, PRIMARY KEY (fldAdr_id))
CREATE TABLE PERSONNE (fldPers_id INTEGER NOT NULL, TYPE_ENTITE VARCHAR(31), fld_nom VARCHAR(255), fld_prenom VARCHAR(255), adresseID INTEGER UNIQUE, PRIMARY KEY (fldPers_id))
CREATE TABLE tbl_cours (fldCours_id INTEGER NOT NULL, fld_codeUE VARCHAR(255), fld_libelleUE VARCHAR(255), fldProf_id INTEGER, PRIMARY KEY (fldCours_id))
ALTER TABLE PERSONNE ADD CONSTRAINT FK_PERSONNE_adresseID FOREIGN KEY (adresseID) REFERENCES tbl_adresse (fldAdr_id)
ALTER TABLE tbl_cours ADD CONSTRAINT FK_tbl_cours_fldProf_id FOREIGN KEY (fldProf_id) REFERENCES PERSONNE (fldPers_id)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
